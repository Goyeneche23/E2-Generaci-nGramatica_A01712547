Inicio ::= Sent Inicio1
Inicio1 ::= Sent Inicio1
Inicio1 ::= ''
Sent ::= StandSent
Sent ::= QuestSent
StandSent ::= StandSentAux FinS
StandSentAux ::= Suj StSaux
StSaux ::= P
StSaux ::= ''
QuestSent ::= QuestSaux FinInt
QuestSaux ::= QsiNo
QuestSaux ::= QAbi
QsiNo ::= V Suj Obj Rest
QAbi ::= QResPre V Suj Obj Rest
QResPre ::= ProInt
QResPre ::= PrepInt
Conj ::= und
Conj ::= oder
Conj ::= aber
FinS ::= .
FinInt ::= ?
PrepInt ::= mit wem
PrepInt ::= an wen
PrepInt ::= worüber
PrepInt ::= auf wen
ProInt ::= wer
ProInt ::= was
ProInt ::= wann
ProInt ::= wo
ProInt ::= warum
ProInt ::= wie
ProInt ::= wohin
ProInt ::= woher
SubSent ::= StandSentAux , SubClause FinS
SubSentAux ::= Suj Obj Rest V
SubClause ::= ConjSub SubSentAux
ConjSub ::= dass
ConjSub ::= weil
ConjSub ::= wenn
ConjSub ::= obwohl
Suj ::= N SujAux
SujAux ::= Conj N SujAux
SujAux ::= ''
A ::= der
A ::= die
A ::= das
A ::= dem
A ::= den
A ::= des
Aind ::= ein
Aind ::= eine
Aind ::= keine
