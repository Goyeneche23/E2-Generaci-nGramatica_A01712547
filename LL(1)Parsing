Obj ::= Oaux Obj1
Obj ::= ''
Obj1 ::= Conj Obj
Obj1 ::= ''
Oaux ::= JustObj
Oaux ::= PrepObj
JustObj ::= A Noun
JustObj ::= Aind Noun
JustObj ::= Noun
PrepObj ::= Prep N
Prep ::= mit
Prep ::= ohne
Prep ::= auf
Prep ::= an
Prep ::= in
Prep ::= für
Prep ::= über
Prep ::= unter
Rest ::= RestAux Rest1
Rest ::= ''
Rest1 ::= Conj RestAux 
Rest1 ::= ''
RestAux ::= Neg Adj
RestAux ::= Adv Adj
Neg ::= ''
Neg ::= nicht
Adv ::= sehr
Adv ::= nie
Adv ::= oft
Adv ::= hier
Adv ::= dort
